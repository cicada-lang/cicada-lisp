(define-datatype bool () ()
  :true bool
  :false bool)

(define true bool:true)
(define false bool:false)

(claim not (-> bool bool))
(define (not x)
  (match x
    (true false)
    (false true)))

(claim add (-> bool bool bool))
(define (add x y)
  (match [x y]
    ([true true] true)
    ([true false] false)
    ([false true] false)
    ([false false] false)))
